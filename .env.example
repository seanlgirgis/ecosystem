# Ecosystem Configuration
# Copy this file to .env and fill in your values

# === API Keys ===
OPENROUTER_API_KEY=your_openrouter_key_here
UPWORK_API_KEY=your_upwork_key_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# === Paths ===
PROJECT_ROOT=C:\ecosystem
STORAGE_PATH=C:\ecosystem\secondbrain\storage

# === Services ===
QDRANT_HOST=localhost
QDRANT_PORT=6333
OLLAMA_HOST=http://localhost:11434

# === Models ===
LOCAL_REASONING_MODEL=qwen2.5:32b-instruct
LOCAL_FAST_MODEL=llama3.1:8b-instruct
LOCAL_LIGHT_MODEL=phi3:mini

# === Feature Flags ===
ENABLE_TELEGRAM=true
ENABLE_CACHING=true
CACHE_SIMILARITY_THRESHOLD=0.90
